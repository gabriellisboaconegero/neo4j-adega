LOAD CSV WITH HEADERS FROM 'file:///{{ table_name }}.csv' AS row
MERGE (x:{{ label }} {id: row.id})
{% if column_names | length > 0 %}
ON CREATE SET
    {% for col in column_names[:-1] %}
    x.{{ col }} = row.{{ col }},
    {% endfor %}
    x.{{ column_names[-1] }} = row.{{ column_names[-1] }}
{% endif %}
RETURN x;
